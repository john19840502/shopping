<div data-hook="admin_user_new_form_header">
  <%= render :partial => 'spree/shared/error_messages', :locals => { :target => @collection_image } %>
  <h1><%= t(:new_image) %></h1>
</div>

<div data-hook="admin_collection_image_new_form">
  <%= form_for [:admin, @collection_image] do |f| %>
    <%= render :partial => 'form', :locals => { :f => f } %>

    <div data-hook="admin_user_new_form_buttons">
      <%=f.submit "Save" %>
    </div>
  <% end %>
</div>
